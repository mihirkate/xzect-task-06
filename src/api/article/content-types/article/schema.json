{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "text",
      "required": true,
      "maxLength": 2000
    },
    "author": {
      "type": "string"
    },
    "published": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "PublishedDate": {
      "type": "datetime",
      "required": true,
      "default": "2024-06-23T18:45:00.000Z"
    },
    "Images": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
